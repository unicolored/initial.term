{
  "name": "initial-sh",
  "version": "0.8.0",
  "description": "The Initial Console for webmasters",
  "main": "initial-sh.bundled.js",
  "module": "initial-sh.js",
  "types": "initial-sh.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:prod": "yarn build ; rollup -c",
    "clean": "rimraf initial.{d.ts,d.ts.map,js,js.map} test/initial.{d.ts,d.ts.map,js,js.map} test/initial_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "check": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --check",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --write",
    "docs": "npm run docs:clean && npm run build:prod && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen && mv ./docs/node_modules ./docs/modules",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/initial-sh.docs.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat initial-sh.bundled.js | gzip -9 | wc -c ; rm initial-sh.bundled.js"
  },
  "dependencies": {
    "@types/mocha": "^10.0.10",
    "lit": "^3.2.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/rollup-plugin-copy": "^0.5.1",
    "@web/rollup-plugin-html": "^2.3.0",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "eslint": "^8.15.0",
    "lit-analyzer": "^1.2.1",
    "postcss": "^8.5.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^4.35.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-summary": "^2.0.1",
    "typescript": "~5.5.0"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "console",
    "initial"
  ],
  "author": "Gilles Hoarau (https://github.com/unicolored)",
  "license": "MIT",
  "files": [
    "*.js",
    "*.ts",
    "*.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/unicolored/initial.term.git"
  },
  "customElements": "custom-elements.json",
  "packageManager": "yarn@4.8.1"
}
